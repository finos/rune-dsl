			<xsd:documentation>Identifier for tri-party netting set.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:token">
				<xsd:attribute name="triPartySetIdScheme"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="TriPartyInformation">
		<xsd:annotation>
			<xsd:documentation>Properties relating to a tri-party repo</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="agentId" type="TriPartyAgentId">
				<xsd:annotation>
					<xsd:documentation>Identifier for the tri-party agent (ideally, a CRDS code or LEI).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="setId" type="TriPartySetId" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Internal identifier for tri-party netting set.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
