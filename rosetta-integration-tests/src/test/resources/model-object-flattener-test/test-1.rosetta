namespace test.tabulator

type Root:
    r1 string (0..1)
    foo Foo (0..1)
    bar Bar (0..*)

type Foo:
    f1 string (0..1)
    bar Bar (0..1)
        [metadata reference]

type Bar:
    [metadata key]
    b1 string (0..1)
    b2 string (0..*)
    b3 string (0..*)
        [metadata scheme]

func Data:
    output:
        result Root (1..1)

    set result:
        Root {
            r1: "VALUE1",
            foo: Foo {
                f1: "VALUE2",
                bar: Bar {
                    b1: "VALUE3",
                    b3: ["VALUE4"],
                    ...
            }},
            bar: [
                Bar {
                    b1: "VALUE5",
                    b2: ["VALUE6", "VALUE7"],
                    ...
                },
                Bar {
                    b1: "VALUE8",
                    b2: ["VALUE9", "VALUE10"],
                    b3: ["VALUE11", "VALUE12"],
                }
            ]
        }
